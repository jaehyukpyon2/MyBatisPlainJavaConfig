<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.example.HRMapper">
  <select id="selectEmployee" resultType="dao.Employee">
    select *
    from Employees
    EMPLOYEE_ID = #{id}
  </select>

  <select id="selectAll" resultType="dao.Employee">
    select *
    from employees
  </select>

  <select id="selectOne" resultType="dao.Employee">
    select *
    from employees
    where employee_id = #{employeeId}
  </select>
</mapper>